<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<StackPane prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hellojavafx.MainViewController">
    <children>
        <StackPane>
            <children>
                <VBox spacing="10.0">
                    <children>
                        <GridPane prefHeight="330.0" prefWidth="535.0">
                            <children>
                                <Label text="Ваши данные:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                                <Label fx:id="sessionLabel" text="Undefined" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                        <Label text="Справочная информация" GridPane.rowIndex="1" />
                        <Label fx:id="importantData" prefHeight="76.0" prefWidth="170.0" text="При пожаре звонить 01" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <ImageView fx:id="importantDataPic" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                            </children>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="229.0" minWidth="10.0" prefWidth="162.0" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="370.0" minWidth="10.0" prefWidth="318.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="160.0" minHeight="10.0" prefHeight="90.0" vgrow="SOMETIMES" />
                                <RowConstraints maxHeight="257.0" minHeight="10.0" prefHeight="64.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="257.0" minHeight="10.0" prefHeight="179.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                        </GridPane>
                        <StackPane prefHeight="45.0" prefWidth="480.0">
                            <children>
                                <Button id="loginButton" fx:id="logoutButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" prefHeight="25.0" prefWidth="71.0" text="Выход" StackPane.alignment="CENTER_RIGHT" />
                                <Button fx:id="backButton" alignment="TOP_LEFT" contentDisplay="TOP" mnemonicParsing="false" prefHeight="28.0" prefWidth="84.0" text="Назад" StackPane.alignment="CENTER_LEFT" />
                        <Button fx:id="nextButton" alignment="TOP_LEFT" contentDisplay="TOP" layoutX="208.0" layoutY="19.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="84.0" text="Вперед" />
                            </children>
                        </StackPane>
                    </children>
                </VBox>
            </children>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</StackPane>
